provider "aws" {
  region = "us-east-1"  # Change this to your desired region
}

resource "aws_s3_bucket" "my_bucket" {
  bucket = "my-unique-bucket-name"  # Change this to your desired bucket name
 # acl    = "private"                # Access control for the bucket

  tags = {
    Name = "MyBucket"
  }
}
